{
  "extends": ["eslint-config-airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "worker": true
    },
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "rules": {
      "prettier/prettier": "error",
      "semi": 2,
      "func-names": "off",
      "no-plusplus": "off",
      "comma-dangle": "off",
      "import/prefer-default-export": "off",
      "class-methods-use-this": "warn",
      "no-unused-vars": "warn",
      "no-param-reassign": "off",
      "no-unused-expressions": "warn",
      "no-bitwise": "off",
      "default-case":"off",
      "no-continue": "off",
      "no-restricted-syntax": "off",
      "no-underscore-dangle": "off",
      "no-multi-spaces": "off",
      "no-undef": "warn"
    }
  }
